{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "chosen/library",
                "version": "1.8.7",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "chosen"
                },
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.7/chosen_v1.8.7.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jquery_touch_punch/library",
                "version": "0.2.3",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "jquery-ui-touch-punch"
                },
                "dist": {
                    "url": "https://github.com/furf/jquery-ui-touch-punch/archive/master.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "facets_range_widget/library",
                "version": "1.11.3",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "jquery-ui-slider-pips"
                },
                "dist": {
                    "url": "https://github.com/simeydotme/jQuery-ui-Slider-Pips/archive/v1.11.3.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "slick/library",
                "version": "1.8.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "slick"
                },
                "dist": {
                    "url": "https://github.com/kenwheeler/slick/archive/1.8.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "blazy/library",
                "version": "1.8.2",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "blazy"
                },
                "dist": {
                    "url": "https://github.com/dinbror/blazy/archive/master.zip",
                    "type": "zip"
                }
            }
        }
    ],
